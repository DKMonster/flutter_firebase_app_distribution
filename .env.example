# Firebase App Distribution 環境變數範例
# 複製此檔案為 .env 並填入實際數值

# Firebase App IDs (從 Firebase Console 取得)
FIREBASE_APP_ID_DEVELOPMENT=your_development_app_id_here
FIREBASE_APP_ID_STAGING=your_staging_app_id_here
FIREBASE_APP_ID_PRODUCTION=your_production_app_id_here
# 這邊如果沒有用flavor記得可以都切換成相同的憑證

# Firebase Project ID
FIREBASE_PROJECT_ID=your_project_id_here

# 測試群組名稱 (可選)
DEFAULT_TESTER_GROUP_DEV=dev-team
DEFAULT_TESTER_GROUP_STAGING=qa-team
DEFAULT_TESTER_GROUP_PRODUCTION=beta-testers

# 其他選項
ENABLE_DEBUG_OUTPUT=false
AUTO_INCREMENT_VERSION=false